<Window x:Class="Store.PassMissMatch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Store"
        mc:Ignorable="d"
        Title="PassMissMatch" Height="450" Width="800">
    <Grid Background="LightBlue">
        <Label Content="Error: Account creation failed. Password didn't match!" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,70,0,0" FontSize="20" FontWeight="Bold"/>
        <Button Click="PasswordMissMatch_Click" Content="Go back" HorizontalAlignment="Center" Margin="0,240,0,0" VerticalAlignment="Top" Width="70" Height="30"/>
    </Grid>
</Window>
